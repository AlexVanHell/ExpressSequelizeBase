{
	"name": "base-line",
	"version": "0.0.0",
	"private": true,
	"main": "./bin/www",
	"config": {
		"env": "production"
	},
	"scripts": {
		"start": "node ./bin/www",
		"apidoc": "apidoc -e \"(node_modules|public)\" -o public/apidoc",
		"demon": "nodemon ./bin/www",
		"debug": "set DEBUG=base-line:*,sockets:*,controllers:*,util:* && nodemon --inspect ./bin/www",
		"debug-linux": "DEBUG=base-line:*,sockets:*,controllers:*,util:* nodemon --inspect ./bin/www",
		"prod": "NODE_ENV=production node ./bin/www"
	},
	"dependencies": {
		"apidoc": "^0.17.6",
		"bcrypt-nodejs": "0.0.3",
		"bluebird": "^3.5.1",
		"body-parser": "~1.18.2",
		"cookie-parser": "~1.4.3",
		"crypto": "^1.0.1",
		"debug": "^3.1.0",
		"express": "^4.16.3",
		"express-validator": "^5.2.0",
		"joi": "^13.4.0",
		"jsonwebtoken": "^8.3.0",
		"morgan": "~1.9.0",
		"mysql2": "^1.5.3",
		"nodemailer": "^4.6.6",
		"pug": "^2.0.3",
		"sequelize": "^4.37.10",
		"serve-favicon": "^2.5.0"
	},
	"devDependencies": {
		"sequelize-cli": "^4.0.0"
	}
}
